function [col] = mean_power(sim)
    col = zeros(numel(sim),1);
    for i = 1:numel(sim)
        tabin = mean(sim(i).out.compar_in.signals.values);
        tabout = mean(sim(i).out.compar_out.signals.values);
        col(i,1) = tabin(3);
        col(i,1) = tabin(3);
        col(i,1) = tabin(3);
        col(i,1) = tabin(3);
        [tabin(3),tabout(3),tabin(4),tabout(4),tabin(5),tabout(5),mean(sim(i).out.SC.signals.values(:,1))];
    end
end
